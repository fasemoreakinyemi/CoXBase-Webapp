<!DOCTYPE html>
<html lang="{{request.locale_name}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="pyramid web application">
    <meta name="Mandela" content="Search form">
    <link rel="shortcut icon" href="{{request.static_url('webapp:static/pyramid-16x16.png')}}">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
    <title>Coxbase</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.5.1/dist/leaflet.css" integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ==" crossorigin=""/>

    <!-- Custom styles for this scaffold -->
    <link href="{{request.static_url('webapp:static/css/main.css')}}" rel="stylesheet">
    <!-- HTML5 shiv and Respond.js IE8 support of HTML5 elements and media queries -->
  </head>

  <body>

	  {% include 'nav.jinja2' %}
    <!--main-->
    <div class="card_body">
      <div class="well well-lg white">
	      <div class="btn-group_mlva">
		      <button class="res_tab" onclick="window.location.href='{{request.route_url('resMLVA', ID=RepeatNumber.ID)}}';">Result table</button>
  		<button class="sub_det" onclick="window.location.href='{{request.route_url('subMLVA', ID=RepeatNumber.ID)}}';">Submission details</button>
  		<button class="phy_ana" onclick="window.location.href='{{request.route_url('phlMLVA', ID=RepeatNumber.ID)}}';">Phylogenetic analysis</button>
	      </div>
		  <div class="info_mlva_table">
			  <table>
                          <tr>
				  <th></th>
                                  <th>ms01</th>
                                  <th>ms03</th>
                                  <th>ms20</th>
                                  <th>ms21</th>
                                  <th>ms22</th>
                                  <th>ms23</th>
                                  <th>ms24</th>
                                  <th>ms26</th>
                                  <th>ms27</th>
                                  <th>ms28</th>
                                  <th>ms30</th>
                                  <th>ms31</th>
                                  <th>ms33</th>
                                  <th>ms34</th>
			  </tr>
			  <tr id='rz_indicator'> 
				  <td>Repeat number</td>
                                  {% if RepeatNumber.ms01 %}
				  <td class="entry">{{RepeatNumber.ms01}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms03 %}
				  <td class="entry">{{RepeatNumber.ms03}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms20 %}
				  <td class="entry">{{RepeatNumber.ms20}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms21 %}
				  <td class="entry">{{RepeatNumber.ms21}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms22 %}
				  <td class="entry">{{RepeatNumber.ms22}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms23 %}
				  <td class="entry">{{RepeatNumber.ms23}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms24 %}
				  <td class="entry">{{RepeatNumber.ms24}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms26 %}
				  <td class="entry">{{RepeatNumber.ms26}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms27 %}
				  <td class="entry">{{RepeatNumber.ms27}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms28 %}
				  <td class="entry">{{RepeatNumber.ms28}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms30 %}
				  <td class="entry">{{RepeatNumber.ms30}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms31 %}
				  <td class="entry">{{RepeatNumber.ms31}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms33 %}
				  <td class="entry">{{RepeatNumber.ms33}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                                  {% if RepeatNumber.ms34 %}
				  <td class="entry">{{RepeatNumber.ms34}}</td>
				  {% else %}
				  <td class="entry"> Not detected</td>
				  {%- endif %}
                          </tr>
                  </table>
      </div>
      <div style="margin-bottom:20px;">
      <button class="submitMLVA"> Submit result as query to MLVA Database </button>
      </div>
      </div>
      <div class="">
		      <div id="result" class="result_info">
		      </div>
      </div>
      </div>






    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://unpkg.com/leaflet@1.5.1/dist/leaflet.js" integrity="sha512-GffPMF3RvMeYyc1LWMHtK8EbPv0iNZ8/oTtHPx9/cc2ILxQ+u905qIwdpULaqDkyBKgOaB57QTMg7ztg8Jm2Og==" crossorigin=""></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="{{request.static_url('webapp:static/js/mlva_query.js')}}" type="text/javascript"></script>
    <script src="{{request.static_url('webapp:static/js/view_map.js')}}" type="text/javascript"></script>
    <script src="{{request.static_url('webapp:static/js/extras.js')}}" type="text/javascript"></script>
  </body>
</html>

    <!--main-->
    <div class="">
	    <h1 class="banner"> Welcome to coxbase</h1>
      <div class="container">
	      <div class="card">
		    <div id="menu-outer">
  			<div class="table">
				<h3> Charts </h3>
    			<ul id="horizontal-list">
			    <li><button class="my_button" value="SampleYear" onclick="location.href='http://localhost:6543/dashboard/SampleYear'">Year</button></li>
			    <li><button class="my_button" value="SampleHost" onclick="location.href='http://localhost:6543/dashboard/SampleHost'">Host</button></li>
			    <li><button class="my_button" value="TypingID" onclick="location.href='http://localhost:6543/dashboard/TypingID'" >TypingID</button></li>
			    <li><button class="my_button" value="TypingID" onclick="location.href='http://localhost:6543/dashboard/SampleCountry'" >Country</button></li>
			    <li><button class="my_button" value="CountryProvince" onclick="location.href='http://localhost:6543/dashboard/CountryProvince'" >City</button></li>
			</ul>
			</div>
		    </div>
	      </div>
	
	<div class="card">
        <div class="row1">
		<canvas id="barchart"></canvas>
	</div>
	</div>
   
	<div class="card">
			<h3> Geographical Information of Isolates</h3>
			<div class=row1>
			<div id='map' style="float:left;"></div>
			<div id='new-parent' class="info" style="margin-bottom:20pt;">
	</div>
			</div>
	</div>
	<div class="card">
		
		<h3> Genotyping Information</h3 >

		<div class="outer_row">
		<div class="row1" style="float:right; clear:both;">

			<p id="genotype_info">Filter Tree: </p>
			<div class="form-group navbar-form navbar-right" id=""> <!--style="float:right; clear:both;">-->
			<input type="text" id = 'branch_filter' class="form-control" placeholder="Filter branches with ID">
			</div>
			<p id="genotype_info" style> Change tree ID: </p>
			<div class="btn-group" id="genotype_info">
				<button id="Country">Country</button>
  				<button id="Host">Host</button>
			</div>
		<!--<input type="text" id = 'branch_filter' class="form-control" placeholder="Filter branches on">-->
	</div>
	</div>

	<div class="row1">
	<svg id="tree_display" width="258.4"></svg>
	</div>
	</div>
	</div>
 


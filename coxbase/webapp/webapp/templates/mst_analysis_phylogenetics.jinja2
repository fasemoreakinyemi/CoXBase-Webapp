<!DOCTYPE html>
<html lang="{{request.locale_name}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="pyramid web application">
    <meta name="Mandela" content="Search form">
    <link rel="shortcut icon" href="{{request.static_url('webapp:static/pyramid-16x16.png')}}">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
    <title>CoxBase</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.5.1/dist/leaflet.css" integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ==" crossorigin=""/>

    <!-- Custom styles for this scaffold -->
    <link href="{{request.static_url('webapp:static/css/main.css')}}" rel="stylesheet">
    <!-- HTML5 shiv and Respond.js IE8 support of HTML5 elements and media queries -->
  </head>

  <body>

	  {% include 'nav.jinja2' %}
    <!--main-->
    <div class="card_body">
      <div class="well well-lg white">
	      <div class="btn-group_mlva">
		      <button class="res_tab" onclick="window.location.href='{{request.route_url('resMST', ID=results.ID)}}';">Result table</button>
  		<button class="sub_det" onclick="window.location.href='{{request.route_url('subMST', ID=results.ID)}}';">Submission details</button>
  		<button class="phy_ana" onclick="window.location.href='{{request.route_url('phlMST', ID=results.ID)}}';">Phylogenetic analysis</button>
	      </div>
		  <div class="info_mst_table">
			  <table>
                          <tr>
                                  <th>cox2</th>
                                  <th>cox5</th>
                                  <th>cox18</th>
                                  <th>cox20</th>
                                  <th>cox22</th>
                                  <th>cox37</th>
                                  <th>cox51</th>
                                  <th>cox56</th>
                                  <th>cox57</th>
                                  <th>cox61</th>
			  </tr>
			  <tr id='rz_indicator'> 
                                  {% if not results.cox2 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox2 == 90 %}
				  <td class="entry">new cox2</td>
				  {% else %}
				  <td class="entry">{{results.cox2}}</td>
				  {%- endif %}
                                  {% if not results.cox5 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox5 == 90 %}
				  <td class="entry">new cox5</td>
				  {% else %}
				  <td class="entry">{{results.cox5}}</td>
				  {%- endif %}
                                  {% if not results.cox18 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox18 == 90 %}
				  <td class="entry">new cox18</td>
				  {% else %}
				  <td class="entry">{{results.cox18}}</td>
				  {%- endif %}
                                  {% if not results.cox20 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox20 == 90 %}
				  <td class="entry">new cox20</td>
				  {% else %}
				  <td class="entry">{{results.cox20}}</td>
				  {%- endif %}
                                  {% if not results.cox22 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox22 == 90 %}
				  <td class="entry">new cox22</td>
				  {% else %}
				  <td class="entry">{{results.cox22}}</td>
				  {%- endif %}
                                  {% if not results.cox37 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox37 == 90 %}
				  <td class="entry">new cox37</td>
				  {% else %}
				  <td class="entry">{{results.cox37}}</td>
				  {%- endif %}
                                  {% if not results.cox51 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox51 == 90 %}
				  <td class="entry">new cox51</td>
				  {% else %}
				  <td class="entry">{{results.cox51}}</td>
				  {%- endif %}
                                  {% if not results.cox56 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox56 == 90 %}
				  <td class="entry">new cox56</td>
				  {% else %}
				  <td class="entry">{{results.cox56}}</td>
				  {%- endif %}
                                  {% if not results.cox57 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox57 == 90 %}
				  <td class="entry">new cox57</td>
				  {% else %}
				  <td class="entry">{{results.cox57}}</td>
				  {%- endif %}
                                  {% if not results.cox61 %}
				  <td class="entry">Not detected</td>
                                  {% elif results.cox61 == 90 %}
				  <td class="entry">new cox61</td>
				  {% else %}
				  <td class="entry">{{results.cox61}}</td>
				  {%- endif %}
                          </tr>

                  </table>
      </div>
      <div class="btn-group_mlva" style="margin:20px 0;">
      <button class="submitMST"> Comparative analysis using MST Database </button>
      </div>
			      <fieldset>
				      <div class="custom-select" style="width:200px;">
					      <p>Max Distance:</p>
			        <select name="distance" id="distance">
    					<option value="0">0</option>
    					<option value="1">1</option>
    					<option value="2">2</option>
    					<option value="3">3</option>
    					<option value="4">4</option>
    					<option value="5">5</option>
					<option value="xx"> 5+ </option>
  				</select>
			      </fieldset>
		      <div id="result" class="result_info">
		      </div>
      </div>
      </div>






    {% include 'footer.jinja2' %}
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://unpkg.com/leaflet@1.5.1/dist/leaflet.js" integrity="sha512-GffPMF3RvMeYyc1LWMHtK8EbPv0iNZ8/oTtHPx9/cc2ILxQ+u905qIwdpULaqDkyBKgOaB57QTMg7ztg8Jm2Og==" crossorigin=""></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="{{request.static_url('webapp:static/js/mst_query.js')}}" type="text/javascript"></script>
    <script src="{{request.static_url('webapp:static/js/view_map.js')}}" type="text/javascript"></script>
    <script src="{{request.static_url('webapp:static/js/extras.js')}}" type="text/javascript"></script>
  </body>
</html>

<!DOCTYPE html>
<html lang="{{request.locale_name}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="pyramid web application">
    <meta name="Mandela" content="Search form">
    <link rel="shortcut icon" href="{{request.static_url('webapp:static/pyramid-16x16.png')}}">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
    <title>CoxBase</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
   <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.10/css/select2.min.css" rel="stylesheet" />
   <link rel="stylesheet" href="https://unpkg.com/leaflet@1.4.0/dist/leaflet.css"
  integrity="sha512-puBpdR0798OZvTTbP4A8Ix/l+A4dHDD0DGqYW6RQ+9jxkRFclaxxQb/SJAWZfWAkuyeQUytO7+7N4QKrDh+drA=="
  crossorigin=""/>


    <!-- Custom styles for this scaffold -->
    <link href="{{request.static_url('webapp:static/css/main.css')}}" rel="stylesheet">
    <!-- HTML5 shiv and Respond.js IE8 support of HTML5 elements and media queries -->
  </head>

  <body>
	  {% include 'nav.jinja2' %}
	  <form name="subform" action="/webapp/retrieve" method="POST" id="subForm" style="margin-top:50px" enctype=multipart/form-data>
	 <div class="card" style="margin: 0 auto;width: 1200px;margin-bottom:200px;">
		 <p> Please enter submission ID: </p>

	  <div class="form-row" style="margin-top:15px">
   			 <div class="form-group">
				 <input type="text" class="form-control" name="submissionID" id="submissionID" value={{submissionID}}>
    			  </div>
		</div>
	<input type="submit" name="submit" value="submit" id="submit_button">

	  </form>
	  <p style="color:red; margin-top:20px;">{{message}}</p>

	<div id="agreement" style="color:red; margin-top:20px;"></div>
	<div id="success" style={{style}}>
		  <div class="info_mlva_table">
			  <table>
			  <tr> 
				  <td>Submission ID</td>
                                  {% if submission %}
				  <td>{{submission.ID}}</td>
				  {% else %}
				  <td></td>
				  {%- endif %}
                          </tr>
			  <tr> 
				  <td>Submission Time</td>
                                  {% if submission %}
				  <td>{{submission.SubmissionDate}}</td>
				  {% else %}
				  <td></td>
				  {%- endif %}
                          </tr>
			  <tr> 
				  <td>Analysis type</td>
                                  {% if submission %}
				  <td>{{submission.AnalysisType}}</td>
				  {% else %}
				  <td></td>
				  {%- endif %}
			  </tr>
			  <tr> 
				  <td>Result link</td>
                                  {% if submission %}
				  <td><button  onclick={{resultlink}} type="button"><b>view result</b></button></td>
				  {% else %}
				  <td></td>
				  {%- endif %}
			  </tr>
                  </table>
		  </div>
	      </div>
	 </div>


    {% include 'footer.jinja2' %}
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
    <script src="{{request.static_url('webapp:static/js/extras.js')}}" type="text/javascript"></script>
    <!--<script src="{{request.static_url('webapp:static/js/retrieve.js')}}" type="text/javascript"></script>-->

  </body>
</html>
